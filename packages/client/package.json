  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "react-start": "react-scripts start ",
    "dev": "env-cmd --no-override .env npm run react-start",
    "dev:int": "env-cmd --no-override .env.local npm run react-start",
    "int": "env-cmd --no-override .env.integration npm run react-start",
    "build": "NODE_OPTIONS='--max-old-space-size=2048' react-scripts build",
    "build:integration": "env-cmd --no-override .env.integration yarn run build",
    "build:production": "NODE_ENV=production env-cmd --no-override .env.production yarn run build ",
    "test": "jest --coverage --coverageDirectory=output/coverage/jest",
    "test:watch": "jest --watchAll --verbose=false",
    "lint": "eslint . --ext .js,.ts,.tsx"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
