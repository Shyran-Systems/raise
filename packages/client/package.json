{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@raisehq/onboarding": "*",
    "@types/hero-ui": "git+https://herolending:N5dMwpevvwyEe5BQBKqk@bitbucket.org/herodevteam/hero-ui.git#integration",
    "@types/jest": "24.0.15",
    "@types/lodash": "^4.14.135",
    "@types/node": "12.0.8",
    "@types/react": "16.8.22",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "^4.3.1",
    "@types/styled-components": "^4.1.6",
    "@types/validator": "^10.9.0",
    "@types/web3": "^1.0.19",
    "axios": "^0.18.0",
    "bowser": "^2.5.2",
    "daggy": "^1.4.0",
    "env-cmd": "^8.0.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "hero-generator-react": "git+https://herolending:N5dMwpevvwyEe5BQBKqk@bitbucket.org/herodevteam/hero-generator-react.git#integration",
    "hero-ui": "git+https://herolending:N5dMwpevvwyEe5BQBKqk@bitbucket.org/herodevteam/hero-ui.git#integration",
    "hoist-non-react-statics": "^3.3.0",
    "jest-mock": "^23.2.0",
    "js-cookie": "^2.2.0",
    "js-cookies": "^1.0.4",
    "lodash": "4.17.14",
    "mocker-api": "^1.6.6",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "pampy": "0.0.6",
    "polished": "^2.3.3",
    "qs": "^6.6.0",
    "rc-slider": "^8.6.13",
    "react": "^16.8.6",
    "react-copy-to-clipboard": "^5.0.1",
    "react-countdown-now": "^2.1.1",
    "react-device-detect": "^1.7.5",
    "react-dom": "^16.8.6",
    "react-dropzone": "^10.1.5",
    "react-gtm-module": "^2.0.7",
    "react-number-format": "^4.2.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-transition": "^1.3.0",
    "react-scripts": "3.0.1",
    "react-share": "^3.0.0",
    "react-toastify": "^5.3.1",
    "react-virtualized-select": "^3.1.3",
    "react-webcam": "^1.1.1",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.85.0",
    "sinon": "^7.2.3",
    "styled-components": "^4.3.2",
    "typescript": "3.5.2",
    "validator": "^10.11.0",
    "web3": "1.0.0-beta.55",
    "web3-utils": "1.0.0-beta.55"
  },
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "react-start": "react-scripts start ",
    "dev": "env-cmd .env npm run react-start",
    "dev:int": "env-cmd .env.local npm run react-start",
    "int": "env-cmd .env.integration npm run react-start",
    "build": "NODE_OPTIONS='--max-old-space-size=2048' react-scripts build",
    "build:integration": "env-cmd .env.integration yarn run build",
    "build:production": "env-cmd .env.production yarn run build ",
    "lint": "eslint src/**/*.js --fix",
    "test": "jest --coverage --coverageDirectory=output/coverage/jest",
    "test:watch": "jest --watchAll --verbose=false",
    "generator": "plop --plopfile ./node_modules/hero-generator-react/index.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "setupFiles": [
      "./.storybook/__conf__/jestMockConfig.js",
      "./.storybook/__conf__/enzymeConfig.js"
    ],
    "testRegex": "\\.test\\.js$",
    "automock": false,
    "globals": {
      "__TESTS__": true
    },
    "roots": [
      "./src"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/lodash/",
      "<rootDir>/node_modules/material-ui/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/enzyme/",
      "<rootDir>/node_modules/chai/",
      "<rootDir>/node_modules/core-js/",
      "<rootDir>/node_modules/sinon/",
      "<rootDir>/node_modules/uuid/",
      "<rootDir>/node_modules/es6-shim/",
      "<rootDir>/node_modules/react-addons-test-utils/"
    ]
  }
}
