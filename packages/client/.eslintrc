{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/forbid-prop-types": 0,
    "react/jsx-one-expression-per-line": 0,
    "import/prefer-default-export": 0,
    "import/no-extraneous-dependencies": 0,
    "comma-dangle": ["error", "never"],
    "arrow-parens": ["as-needed"]
  }
}
