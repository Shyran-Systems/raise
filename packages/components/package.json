    "env-cmd": "8.0.2",
    "graphql.js": "0.6.1",
    "lodash": "^4.17.15",
    "lodash-es": "^4.17.15",
    "numeral": "^2.0.6",
    "pampy": "^0.0.6",
    "prettier": "^1.19.1",
