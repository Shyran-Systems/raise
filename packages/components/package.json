  "main": "dist/index.js",
  "module": "dist/components.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "env-cmd --no-override .env tsdx watch",
    "dev": "env-cmd --no-override .env tsdx watch",
    "dev:int": "yarn run dev",
    "build": "rm -f tsdx.config.js && env-cmd --no-override .env tsdx build",
    "build:umd": "cp tsdx.umd.config.js tsdx.config.js && env-cmd --no-override .env tsdx build --format umd --name RaiseComponents --input src/index.cards.tsx",
    "test": "env-cmd --no-override .env tsdx test --env=jsdom",
    "lint": "env-cmd --no-override .env tsdx lint"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.23",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "babel-loader": "^8.0.6",
    "typescript": "3.8.3",
    "husky": "^3.1.0",
    "tslib": "^1.10.0"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/lodash": "^4.14.149",
