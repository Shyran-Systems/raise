{
  "name": "@raisehq/pages",
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "@raisehq/components": "*",
    "@raisehq/onboarding": "*",
    "@types/react": "16.9.0",
    "@types/react-dom": "16.9.0",
    "buttercms": "^1.1.5",
    "dompurify": "^1.0.11",
    "env-cmd": "^8.0.2",
    "gatsby": "^2.21.13",
    "gatsby-image": "^2.4.1",
    "gatsby-plugin-env-variables": "^1.0.2",
    "gatsby-plugin-google-tagmanager": "^2.3.1",
    "gatsby-plugin-less": "^3.2.1",
    "gatsby-plugin-manifest": "^2.4.2",
    "gatsby-plugin-offline": "^3.2.1",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.3",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-plugin-sharp": "^2.6.1",
    "gatsby-plugin-styled-components": "^3.3.1",
    "gatsby-plugin-typescript": "^2.4.2",
    "gatsby-source-buttercms": "^2.1.1",
    "gatsby-source-filesystem": "^2.3.1",
    "gatsby-transformer-sharp": "^2.5.1",
    "htmr": "^0.8.7",
    "js-cookie": "^2.2.0",
    "lodash": "4.17.14",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "react-scripts": "3.4.0",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "eslint": "^6.2.1",
    "eslint-config-airbnb-typescript": "^4.0.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.14.3",
    "jest": "^24.9.0",
    "react-use-cookie": "^1.0.2",
    "ts-jest": "^24.0.2"
  },
  "scripts": {
    "build": "env-cmd --no-override .env.local gatsby build",
    "develop": "env-cmd --no-override .env.local gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
